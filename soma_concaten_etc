a = input()
b = input()
#letra_a
print (float(a) + float(b))
#letra_b
print(f'{float(a):.1f}' + b)
#letra_c
print(float(a) * float(b))
#letra_d
multi_str = f'{float(a):.1f}' * int(b)
print(multi_str)
#letra_e
print(float(a) / float(b))
#letra_f divisao inteira
print(float(a) // float(b))
#letra_g
print(float(a) ** float(b))
#letra_h
print(float(a) % float(b))
